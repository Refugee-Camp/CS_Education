<h2>Create new duck</h2>

<%= form_for @duck do |f|%>
  <%= f.label :name%>
  <%= f.text_field :name%>

  <%= f.label :description%>
  <%= f.text_field :description%>

  <%= f.label :student_id%>
  <%= f.collection_select(:student_id, Student.all, :id, :name, promt: true )%>
  <%= f.submit %>

<% end %>
